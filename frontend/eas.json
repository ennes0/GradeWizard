{
  "cli": {
    "version": ">= 5.9.1",
    "appVersionSource": "remote",
    "requireCommit": true
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "buildConfiguration": "Debug",
        "resourceClass": "m-medium"
      },
      "env": {
        "EXPO_USE_YARN": "false"
      },
      "node": "18.17.0"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "resourceClass": "m-medium",
        "buildConfiguration": "Release"
      }
    },
    "production": {
      "autoIncrement": true,
      "distribution": "store",
      "ios": {
        "resourceClass": "m-medium",
        "buildConfiguration": "Release",
        "image": "latest",
        "bundleIdentifier": "com.enesoy.gradwizard"
      },
      "appStoreConnect": {
        "appleId": "6741734068",
        "ascAppId": "6741734068",
        "sku": "EX1739107076939"
      }
    }
  }
}



