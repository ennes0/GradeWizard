{
  "expo": {
    "name": "Grad Wizard",
    "slug": "gradwizard",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/logo0.png",
      "resizeMode": "contain",
      "backgroundColor": "#0e1b0d"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.enesoy.gradwizard",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": ["remote-notification"],
        "NSCameraUsageDescription": "This app uses the camera to let you pick a profile photo.",
        "NSPhotoLibraryUsageDescription": "This app uses the photo library to let you pick a profile photo."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo0.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo.png"
    },
    "plugins": [
      "expo-router",
      "expo-localization",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/logo.png",
          "color": "#51B402",
          "sounds": ["./assets/sounds/notification.wav"]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "notification": {
      "icon": "./assets/images/logo.png",
      "color": "#51B402",
      "iosDisplayInForeground": true,
      "androidMode": "default",
      "androidCollapsedTitle": "Grade Wizard",
      "sound": "notification.wav"
    },
    "locales": {
      "tr": "./translations/tr.json",
      "en": "./translations/en.json"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c7af8250-2584-41a8-bf2c-1df4d81266bf"
      }
    }
  }
}
