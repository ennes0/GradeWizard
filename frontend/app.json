{
  "expo": {
    "name": "Grad Wizard",
    "slug": "gradwizard",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/logo0.png",
      "resizeMode": "contain",
      "backgroundColor": "#0e1b0d"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.enesoy.gradwizard",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": [
          "remote-notification"
        ],
        "NSCameraUsageDescription": "This app uses the camera to let you pick a profile photo.",
        "NSPhotoLibraryUsageDescription": "This app uses the photo library to let you pick a profile photo."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo0.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.enesoy.gradwizard"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/logo.png",
          "color": "#51B402"
        }
      ],
      [
        "expo-ads-admob",
        {
          "userTrackingPermission": "This identifier will be used to deliver personalized ads to you.",
          "androidAdUnitId": "ca-app-pub-7510384238146981~7946410411",
          "iosAdUnitId": "ca-app-pub-7510384238146981~7946410411"
        }
      ],
      [
        "react-native-google-mobile-ads",
        {
          "iosAppId": "ca-app-pub-7510384238146981~7946410411",
          "androidAppId": "ca-app-pub-7510384238146981~7946410411"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "notification": {
      "icon": "./assets/images/logo.png",
      "color": "#51B402",
      "iosDisplayInForeground": true,
      "androidMode": "default",
      "androidCollapsedTitle": "Grade Wizard"
    },
    "locales": {
      "tr": "./translations/tr.json",
      "en": "./translations/en.json"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c7af8250-2584-41a8-bf2c-1df4d81266bf"
      }
    }
  }
}
