{
  "expo": {
    "name": "Grade Wizard",
    "slug": "gwizard",
    "version": "1.0.0",
    "orientation": "portrait",
    "owner": "ennoo",
    "icon": "./assets/images/loggo2.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/loggo2.png",
      "resizeMode": "contain",
      "backgroundColor": "#23720B"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.enesoy.gradwizard",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": [
          "remote-notification"
        ],
        "NSCameraUsageDescription": "This app uses the camera to let you pick a profile photo.",
        "NSPhotoLibraryUsageDescription": "This app uses the photo library to let you pick a profile photo."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/loggo2.png",
        "backgroundColor": "#23720B"
      },
      "package": "com.enesoy.gradwizard"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/loggo2.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "bfd6db33-c23c-40a9-aa02-3967402f33e5"
      }
    },
    "experiments": {
      "typedRoutes": true
    },
    "notification": {
      "icon": "./assets/images/loggo.png",
      "color": "#51B402",
      "iosDisplayInForeground": true,
      "androidMode": "default",
      "androidCollapsedTitle": "Grade Wizard"
    },
    "locales": {
      "tr": "./translations/tr.json",
      "en": "./translations/en.json"
    }
  }
}
